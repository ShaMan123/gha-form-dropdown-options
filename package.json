{
	"name": "gha-form-dropdown-options",
	"version": "0.0.1",
	"description": "A github action populating options for an issue forms dropdown",
	"license": "MIT",
	"author": "ShaMan123",
	"repository": "https://github.com/ShaMan123/gha-form-dropdown-options",
	"type": "module",
	"keywords": [
		"actions",
		"issue",
		"form",
		"issue forms",
		"version",
		"versions",
		"dropdown",
		"list",
		"options"
	],
	"main": "dist/main.cjs",
	"scripts": {
		"start": "npm run build -- -w",
		"build": "rollup -c",
		"test": "mocha --require source-map-support/register test/index.test.js",
		"format": "prettier --write src/*.js src/**/*.js"
	},
	"dependencies": {
		"@actions/core": "^1.10.0",
		"@actions/github": "^5.1.1",
		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@babel/core": "^7.8.6",
		"@babel/node": "^7.8.4",
		"@babel/preset-env": "^7.12.11",
		"@octokit/webhooks-types": "^6.3.6",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/lodash": "^4.14.186",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.7.21",
		"babel-jest": "^29.0.1",
		"core-js": "3",
		"dotenv": "^16.0.2",
		"mocha": "^10.0.0",
		"prettier": "^2.7.1",
		"regenerator-runtime": "^0.13.3",
		"rollup": "^2.78.1",
		"rollup-plugin-node-externals": "^4.1.1",
		"source-map-support": "^0.5.21"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"useBuiltIns": "usage",
					"corejs": 3
				}
			]
		]
	},
	"prettier": {
		"trailingComma": "none",
		"singleQuote": true,
		"tabWidth": 2
	},
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/ShaMan123"
	}
}
